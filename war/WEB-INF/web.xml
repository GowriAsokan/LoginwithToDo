<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>SignupServlet</servlet-name>
		<servlet-class>jdosample.SignupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SignupServlet</servlet-name>
		<url-pattern>/SignupServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LoginpageServlet</servlet-name>
		<servlet-class>jdosample.LoginpageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginpageServlet</servlet-name>
		<url-pattern>/LoginpageServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ListpageServlet</servlet-name>
		<servlet-class>jdosample.ListpageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListpageServlet</servlet-name>
		<url-pattern>/ListpageServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RetrieveListServlet</servlet-name>
		<servlet-class>jdosample.RetrieveListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RetrieveListServlet</servlet-name>
		<url-pattern>/retrievelistservlet</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>loginpage.jsp</welcome-file>
	</welcome-file-list>
</web-app>
